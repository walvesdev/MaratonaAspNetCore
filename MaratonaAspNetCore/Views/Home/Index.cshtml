@model List<Produto>
@{
    ViewData["Title"] = "Home Page";
}
    <div class="container">
        <table class="table">
            <thead>
                <tr>
                    <th scope="col">ID</th>
                    <th scope="col">Nome</th>
                    <th scope="col">Tipo</th>
                    <th scope="col">Valor</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <th scope="row">@item.Id</th>
                        <td>@item.Nome</td>
                        <td>@item.Tipo.Nome</td>
                        <td>@item.Valor</td>
                    </tr>
                }

            </tbody>
        </table>
    </div>